{
  "name": "reactjs-material-vite-starter",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cy:open": "cypress open --e2e --browser edge",
    "cy:run": "cypress run --e2e --browser edge",
    "cy:open-ct": "cypress open --component --browser edge",
    "cy:run-ct": "cypress run --component --browser edge",
    "cypress:run": "cypress run --config video=false --component"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cypress/react": "^8.0.1",
    "@cypress/vite-dev-server": "^5.0.7",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.3",
    "@types/node": "^20.11.27",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "cypress": "^13.9.0",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "packageManager": "yarn@4.1.1"
}
